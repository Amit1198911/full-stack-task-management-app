{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "script-src 'self' 'unsafe-inline' 'wasm-unsafe-eval';"
        },
        {
          "key": "x-forwarded-proto",
          "value": "http"
        }
      ]
    }
  ]
}
